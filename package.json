{
  "name": "kantab",
  "version": "0.0.0",
  "description": "Kanban board application with Moleculer microservices & Vue.js",
  "scripts": {
    "dev": "moleculer-runner --env --repl --hot backend/services",
    "start": "moleculer-runner --env",
    "build": "vue-cli-service build",
    "ci": "jest -c jest.backend.config.js --watch",
    "coverall": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "test": "jest -c jest.backend.config.js --coverage --no-cache",
    "lint": "eslint backend && vue-cli-service lint",
    "deps": "npm-check -u",
    "postdeps": "npm test",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e"
  },
  "keywords": [
    "microservices",
    "moleculer"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/icebob/kantab.git"
  },
  "author": "Icebob",
  "devDependencies": {
    "@vue/cli-plugin-babel": "3.0.1",
    "@vue/cli-plugin-e2e-cypress": "3.0.1",
    "@vue/cli-plugin-eslint": "3.0.1",
    "@vue/cli-plugin-pwa": "3.0.1",
    "@vue/cli-plugin-unit-jest": "3.0.1",
    "@vue/cli-service": "3.0.1",
    "@vue/eslint-config-airbnb": "3.0.1",
    "@vue/test-utils": "1.0.0-beta.20",
    "axios": "0.18.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "23.4.2",
    "coveralls": "3.0.2",
    "es6-promise": "4.2.4",
    "eslint": "5.5.0",
    "eslint-plugin-vue": "5.0.0-beta.3",
    "jest": "23.5.0",
    "jest-cli": "23.5.0",
    "js-cookie": "2.2.0",
    "moleculer-repl": "0.5.2",
    "node-sass": "4.9.3",
    "npm-check": "5.8.0",
    "register-service-worker": "1.5.2",
    "sass-loader": "7.1.0",
    "vue": "2.5.17",
    "vue-cli-plugin-better-webpack-bundle-analyzer": "0.1.2",
    "vue-router": "3.0.1",
    "vue-template-compiler": "2.5.17",
    "vuex": "3.0.1",
    "vuex-router-sync": "5.0.0",
    "webpack-bundle-analyzer": "2.13.1",
    "webpack-dev-middleware": "3.2.0",
    "webpack-hot-middleware": "2.23.1"
  },
  "dependencies": {
    "bcrypt": "3.0.0",
    "bluebird": "3.5.2",
    "cookie": "0.3.1",
    "cross-env": "5.2.0",
    "dotenv": "6.0.0",
    "glob": "7.1.3",
    "helmet": "3.13.0",
    "jsonwebtoken": "8.3.0",
    "moleculer": "0.13.2",
    "moleculer-db": "0.7.5",
    "moleculer-db-adapter-mongo": "0.4.1",
    "moleculer-mail": "1.2.1",
    "moleculer-web": "0.8.1",
    "moment": "2.22.2",
    "passport": "0.4.0",
    "passport-facebook": "2.1.1",
    "passport-github": "1.1.0",
    "passport-google-oauth20": "1.0.0",
    "pug": "2.0.3"
  },
  "engines": {
    "node": ">= 8.x.x"
  },
  "jest": {
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "rootDir": "./",
    "roots": [
      "./test"
    ]
  }
}
