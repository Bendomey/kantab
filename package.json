{
  "name": "kantab",
  "version": "0.0.0",
  "description": "Kanban board application with microservices",
  "scripts": {
    "dev": "moleculer-runner --repl --hot services",
    "start": "moleculer-runner",
    "ci": "jest --watch",
    "test": "jest --coverage",
    "lint": "eslint services",
    "deps": "npm-check -u"
  },
  "keywords": [
    "microservices",
    "moleculer"
  ],
  "author": "Icebob",
  "devDependencies": {
    "eslint": "5.3.0",
    "jest": "23.5.0",
    "jest-cli": "23.5.0",
    "moleculer-repl": "0.5.1",
    "npm-check": "5.8.0"
  },
  "dependencies": {
    "bluebird": "3.5.1",
    "cross-env": "5.2.0",
    "dotenv": "6.0.0",
    "glob": "7.1.2",
    "jsonwebtoken": "8.3.0",
    "moleculer": "0.13.1",
    "moleculer-db": "0.7.5",
    "moleculer-db-adapter-mongo": "0.4.1",
    "moleculer-mail": "1.2.1",
    "moleculer-web": "0.8.1",
    "moment": "2.22.2"
  },
  "engines": {
    "node": ">= 8.x.x"
  },
  "jest": {
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "rootDir": "./services",
    "roots": [
      "../test"
    ]
  }
}
