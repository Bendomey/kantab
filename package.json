  "author": "Icebob",
  "devDependencies": {
    "eslint": "5.3.0",
    "jest": "23.5.0",
    "jest-cli": "23.5.0",
    "moleculer-repl": "0.5.1",
    "npm-check": "5.8.0",
    "@vue/cli-plugin-babel": "^3.0.0",
    "@vue/cli-plugin-e2e-cypress": "^3.0.0",
    "@vue/cli-plugin-eslint": "^3.0.0",
    "@vue/cli-plugin-pwa": "^3.0.0",
    "@vue/cli-plugin-unit-jest": "^3.0.0",
