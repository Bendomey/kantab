{
  "name": "kantab",
  "version": "0.1.0",
  "description": "Kanban board application with Moleculer microservices & Vue.js",
  "scripts": {
    "dev": "moleculer-runner --env --repl --hot backend/services",
    "start": "moleculer-runner --env",
    "now-start": "cross-env NEDB_FOLDER=data moleculer-runner --env backend/services",
    "build": "vue-cli-service build",
    "lint": "eslint backend && vue-cli-service lint",
    "deps": "npm-check -u",
    "postdeps": "npm test",
    "ci": "jest -c jest.backend.config.js --watch",
    "ci:e2e": "cross-env TEST_E2E=open MONGO_URI=mongodb://localhost/kantab-e2e moleculer-runner --env backend/services",
    "test": "npm run test:backend && npm run test:e2e",
    "test:backend": "jest -c jest.backend.config.js --coverage --no-cache",
    "test:frontend": "vue-cli-service test:unit",
    "test:e2e": "cross-env TEST_E2E=run MONGO_URI=mongodb://localhost/kantab-e2e moleculer-runner --env backend/services",
    "coverall": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "keywords": [
    "microservices",
    "moleculer"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/icebob/kantab.git"
  },
  "author": "Icebob",
  "devDependencies": {
    "@vue/cli-plugin-babel": "3.0.4",
    "@vue/cli-plugin-e2e-cypress": "3.0.4",
    "@vue/cli-plugin-eslint": "3.0.4",
    "@vue/cli-plugin-pwa": "3.0.4",
    "@vue/cli-plugin-unit-jest": "3.0.4",
    "@vue/cli-service": "3.0.4",
    "@vue/eslint-config-airbnb": "3.0.4",
    "@vue/test-utils": "1.0.0-beta.20",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "23.6.0",
    "coveralls": "3.0.2",
    "es6-promise": "4.2.5",
    "eslint": "5.6.1",
    "eslint-plugin-vue": "5.0.0-beta.3",
    "fakerator": "0.3.0",
    "jest": "23.6.0",
    "jest-cli": "23.6.0",
    "js-cookie": "2.2.0",
    "moleculer-repl": "0.5.2",
    "node-sass": "4.9.3",
    "npm-check": "5.9.0",
    "register-service-worker": "1.5.2",
    "sass-loader": "7.1.0",
    "sweetalert": "2.1.0",
    "vue": "2.5.17",
    "vue-cli-plugin-better-webpack-bundle-analyzer": "0.1.2",
    "vue-izitoast": "1.1.0",
    "vue-router": "3.0.1",
    "vue-template-compiler": "2.5.17",
    "vuex": "3.0.1",
    "vuex-router-sync": "5.0.0",
    "webpack-bundle-analyzer": "3.0.2",
    "webpack-dev-middleware": "3.4.0",
    "webpack-hot-middleware": "2.24.2",
    "yaqrcode": "0.2.1"
  },
  "dependencies": {
    "axios": "0.18.0",
    "bcrypt": "3.0.0",
    "bluebird": "3.5.2",
    "cookie": "0.3.1",
    "cross-env": "5.2.0",
    "dotenv": "6.0.0",
    "execa": "1.0.0",
    "glob": "7.1.3",
    "helmet": "3.13.0",
    "jsonwebtoken": "8.3.0",
    "moleculer": "0.13.3",
    "moleculer-console-tracer": "0.2.0",
    "moleculer-db": "0.7.5",
    "moleculer-db-adapter-mongo": "0.4.1",
    "moleculer-mail": "1.2.1",
    "moleculer-web": "0.8.2",
    "moment": "2.22.2",
    "passport": "0.4.0",
    "passport-facebook": "2.1.1",
    "passport-github": "1.1.0",
    "passport-google-oauth20": "1.0.0",
    "pug": "2.0.3",
    "speakeasy": "2.0.0"
  },
  "engines": {
    "node": ">= 8.x.x"
  },
  "now": {
    "alias": "kantab"
  },
  "jest": {
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "rootDir": "./",
    "roots": [
      "./test"
    ]
  }
}
