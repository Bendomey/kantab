{
  "name": "kantab",
  "version": "0.0.0",
  "description": "Kanban board application with microservices",
  "scripts": {
    "dev": "moleculer-runner --envfile=backend/.env --repl --hot backend/services",
    "start": "moleculer-runner --envfile=backend/.env",
    "ci": "jest --watch",
    "test": "jest --coverage",
    "lint": "eslint backend frontend",
    "deps": "npm-check -u"
  },
  "keywords": [
    "microservices",
    "moleculer"
  ],
  "author": "Icebob",
  "devDependencies": {
    "eslint": "5.3.0",
    "jest": "23.5.0",
    "jest-cli": "23.5.0",
    "moleculer-repl": "0.5.1",
    "npm-check": "5.8.0"
  },
  "dependencies": {
    "bcrypt": "3.0.0",
    "bluebird": "3.5.1",
    "cookie": "0.3.1",
    "cross-env": "5.2.0",
    "dotenv": "6.0.0",
    "glob": "7.1.2",
    "helmet": "3.13.0",
    "jsonwebtoken": "8.3.0",
    "moleculer": "0.13.1",
    "moleculer-db": "0.7.5",
    "moleculer-db-adapter-mongo": "0.4.1",
    "moleculer-mail": "1.2.1",
    "moleculer-web": "0.8.1",
    "moment": "2.22.2",
    "passport": "0.4.0",
    "passport-facebook": "2.1.1",
    "passport-github": "1.1.0",
    "passport-google-oauth20": "1.0.0",
    "passport-twitter": "1.0.4",
    "pug": "2.0.3"
  },
  "engines": {
    "node": ">= 8.x.x"
  },
  "jest": {
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "rootDir": "./",
    "roots": [
      "./test"
    ]
  }
}
